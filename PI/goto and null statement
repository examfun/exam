declare
  a number := 30;
begin
  <<loopstart>>
  while a < 50 loop
    DBMS_OUTPUT.PUT_LINE('VALUE OF a: ' || a);
    a := a + 1;
    if a < 50 then
      goto loopstart;
    end if;
  end loop;
end;
/

 null statement 

  CREATE OR REPLACE PROCEDURE divide_if(
  p_numerator   IN  NUMBER,
  p_denominator IN  NUMBER,
  p_result      OUT NUMBER
)
IS
BEGIN
  IF p_denominator = 0 THEN
    p_result := NULL;  -- Handling division by zero by setting result to NULL
  ELSE
    p_result := p_numerator / p_denominator;  -- Perform division
  END IF;
END;
/
