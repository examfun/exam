INSERT INTO employees VALUES (3, 'Shivam', 17, 'Mahim', 20000);
INSERT INTO employees VALUES (4, 'Prashant', 18, 'Sion', 22000);
INSERT INTO employees VALUES (5, 'Sam', 18, 'Bandra', 18000);
select *from employees
  IF SQL%NOTFOUND THEN
    DBMS_OUTPUT.PUT_LINE('No employees selected');
  ELSEif sql% found then
    total_rows := SQL%ROWCOUNT;
    DBMS_OUTPUT.PUT_LINE(total_rows || ' employees selected');
  END IF;
END;
/
select *from employees
