implicit
CREATE TABLE employees (
  id NUMBER PRIMARY KEY,
  name VARCHAR2(20),
  age NUMBER,
  address VARCHAR2(20),
  salary NUMBER
);
INSERT INTO employees VALUES (1, 'Durgesh', 18, 'Matunga', 25000);
